{
  "headers": [
    {
      "source": "/:path*.(js|css|png|jpg|jpeg|svg|webp|avif)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/:path*.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, s-maxage=600, stale-while-revalidate=30" }
      ]
    }
  ]
}

